{
  "name": "smartql",
  "version": "11.0.0-rc.5",
  "description": "A GraphQL implementation for interacting with EOSIO Antelope based blockchains.",
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write readme.md && prettier --write changelog.md",
    "test": "npm run test:eslint && npm run test:prettier",
    "test:api": "ALLOW_MISSING_COVERAGE=1 coverage-node test/index.test.mjs",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "prepublishOnly": "npm test"
  },
  "files": [
    "blockchain",
    "eosio_types",
    "graphql_input_types",
    "graphql_object_types",
    "build_graphql_fields_from_abis.js",
    "eos_abi_to_graphql_ast.js",
    "mutation_resolver.js",
    "push_serialized_transaction.js",
    "push_transaction_rpc.js",
    "push_transaction.js",
    "query_resolver.js",
    "serialize_transaction.js",
    "smartql.js"
  ],
  "engines": {
    "node": ">=15"
  },
  "sideEffects": false,
  "exports": {
    "./blockchain/index.js": "./blockchain/index.js",
    "./eosio_types/index.js": "./eosio_types/index.js",
    "./build_graphql_fields_from_abis.js": "./build_graphql_fields_from_abis.js",
    "./eos_abi_to_graphql_ast.js": "./build_graphql_fields_from_abis.js",
    "./mutation_resolver.js": "./mutation_resolver.js",
    "./package.json": "./package.json",
    "./push_serialized_transaction.js": "./push_serialized_transaction.js",
    "./push_transaction_rpc.js": "./push_transaction_rpc.js",
    "./push_transaction.js": "./push_transaction.js",
    "./query_resolver.js": "./query_resolver.js",
    "./serialize_transaction.js": "./serialize_transaction.js",
    ".": "./smartql.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pur3miish/smartql.git"
  },
  "browserslist": [
    "> 0.5%, not OperaMini all, not IE > 0, not dead"
  ],
  "keywords": [
    "blockchain",
    "graphql",
    "smart",
    "contract",
    "bitcoin",
    "eosio",
    "eos",
    "query",
    "language"
  ],
  "author": "pur3miish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pur3miish/smartql/issues"
  },
  "homepage": "https://github.com/pur3miish/smartql#readme",
  "devDependencies": {
    "coverage-node": "^8.0.0",
    "eslint": "^8.25.0",
    "eslint-config-env": "^23.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^4.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "isomorphic-fetch": "^3.0.0",
    "jsdoc-md": "^11.0.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7",
    "test-director": "^10.0.0"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "dependencies": {
    "base58-js": "*",
    "eos-ecc": "^3.1.0",
    "eosio-wasm-js": "^2.1.0-rc",
    "ripemd160-js": "*"
  }
}
